    "config": { "secure-http": false },
    "require": {
        "nette/application": "^2.4",
        "nette/bootstrap": "^2.4",
        "nette/caching": "^2.4",
        "nette/di": "^2.4",
        "nette/finder": "^2.4",
        "nette/forms": "^2.4",
        "nette/http": "^2.4",
        "nette/mail": "^2.4",
        "nette/robot-loader": "^2.4",
        "nette/safe-stream": "^2.3",
        "nette/security": "^2.4",
        "nette/utils": "^2.4",
        "latte/latte": "^2.4",
        "tracy/tracy": "^2.4",
        "janmarek/webloader": ">= v2.5.0",
        "joseki/webloader-filters": "v1.1",
        "oyejorge/less.php": "v1.7.0.*",
        "dusank/knapsack": "^10.0",
