        "wame/ComponentModule": ">=0.0.1",
        "wame/Utils": ">=0.0.1",
        "wame/ChameleonComponentsListControl": ">=0.0.1",
        "wame/RouterModule": ">=0.0.1",
        "wame/RestApiModule": ">=0.0.1",
        "wame/GeneralSettingsPlugin": ">=0.0.1"
    },
    "require-dev": {
        "wame/DoctrineFixtures": ">=0.0.1"
    },
    "softRequire": {
        "wame/AdminModule": ">=0.0.1"
    },
    "autoload": {
        "classmap": [
            "."
        ]
    },
    "type": "wame-module"
}
